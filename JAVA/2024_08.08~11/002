//2. a와 b 출력하기

import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt();
        int b = sc.nextInt();

        System.out.println("a = "+a);
        System.out.println("b = "+b);

    }
}

//a=4,b-5를 입력하면 원하는 값이 출력됨
